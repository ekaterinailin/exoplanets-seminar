   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "def amplification(ml):\n",
    "    u = 1/np.sqrt(ml)\n",
    "    return (u + 2)/ np.sqrt(u**2 + 4)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "ml = np.linspace(0.01,20,300)\n",
    "ampl = amplification(ml)\n"
   ]
  },
